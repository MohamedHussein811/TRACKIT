{
  "expo": {
    "name": "TrackIt",
    "slug": "expo-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/adaptive-icon.png",
    "scheme": "trackit",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.trackitapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.trackitapp"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "b7d820bb-b12f-4fb3-b5f1-32e79d183971"
      }
    },
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/b7d820bb-b12f-4fb3-b5f1-32e79d183971",
      "enabled": true,
      "fallbackToCacheTimeout": 0
    },
    "plugins": [
      "expo-router",
      "expo-camera",
      "expo-barcode-scanner",
      "expo-image-picker",
      "expo-media-library"
    ]
  }
}
